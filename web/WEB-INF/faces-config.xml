<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <navigation-rule>
        <from-view-id>/edit/profesionalauditoredit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>canceledit</from-outcome>
            <to-view-id>/cuentausuario.xhtml</to-view-id>
            <redirect>true</redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/cuentausuario.xhtml</to-view-id>
            <redirect>true</redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/edit/empresaconsultoraedit.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>canceledit</from-outcome>
            <to-view-id>/cuentausuario.xhtml</to-view-id>
            <redirect>true</redirect>
        </navigation-case>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/cuentausuario.xhtml</to-view-id>
            <redirect>true</redirect>
        </navigation-case>
    </navigation-rule>
</faces-config>
